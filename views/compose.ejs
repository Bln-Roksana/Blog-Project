<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label class="compose-labels" for="title">Title</label>
    <input id="title" class="form-control" type="text" name="titleToPublish" placeholder="Type here">
  </div>
  <div class="form-group">
    <label class="compose-labels"for="post">Post</label>
    <textarea id="post" class="form-control" type="text" name="textToPublish" placeholder="Type here" rows="3"></textarea>
  </div>


  <button type="submit" name="button" class="btn btn-info">Publish</button>
</form>

<%- include("partials/footer"); -%>
